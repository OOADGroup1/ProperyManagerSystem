<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.propertysys.bean.InstallRecord" table="InstallRecord" schema="prtydb">
        <composite-id mapped="true" class="com.propertysys.bean.InstallRecordPK">
            <key-property name="equipSeriesId">
                <column name="equip_series_id" sql-type="int(11)"/>
            </key-property>
            <key-property name="spareSeriesId">
                <column name="spare_series_id" sql-type="int(11)"/>
            </key-property>
        </composite-id>
        <property name="installDate">
            <column name="install_date" sql-type="timestamp"/>
        </property>
    </class>
</hibernate-mapping>