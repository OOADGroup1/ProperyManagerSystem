<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.propertysys.bean.EquipItem" table="EquipItem" schema="prtydb">
        <id name="equipSeriesId">
            <column name="equip_series_id" sql-type="int(11)"/>
        </id>
        <property name="catlogId">
            <column name="catlog_id" sql-type="int(11)"/>
        </property>
        <property name="equipId">
            <column name="equip_id" sql-type="int(11)"/>
        </property>
        <property name="equipStatus">
            <column name="equip_status" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>