<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.propertysys.bean.Equipment" table="Equipment" schema="prtydb">
        <composite-id mapped="true" class="com.propertysys.bean.EquipmentPK">
            <key-property name="catlogId">
                <column name="catlog_id" sql-type="int(11)"/>
            </key-property>
            <key-property name="equipId">
                <column name="equip_id" sql-type="int(11)"/>
            </key-property>
        </composite-id>
        <property name="catlogType">
            <column name="catlog_type" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="equipDesc">
            <column name="equip_desc" sql-type="text" not-null="true"/>
        </property>
        <property name="equipPrice">
            <column name="equip_price" sql-type="float" precision="-1" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>