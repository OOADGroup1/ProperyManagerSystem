<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.propertysys.bean.EquipRentRecord" table="EquipRentRecord" schema="prtydb">
        <composite-id mapped="true" class="com.propertysys.bean.EquipRentRecordPK">
            <key-property name="employeeId">
                <column name="employee_id" sql-type="int(11)"/>
            </key-property>
            <key-property name="equipSeriesId">
                <column name="equip_series_id" sql-type="int(11)"/>
            </key-property>
        </composite-id>
        <property name="rentDate">
            <column name="rent_date" sql-type="timestamp"/>
        </property>
        <property name="rentAction">
            <column name="rent_action" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>