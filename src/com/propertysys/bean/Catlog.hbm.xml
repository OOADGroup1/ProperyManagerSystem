<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.propertysys.bean.Catlog" table="Catlog" schema="prtydb">
        <id name="catlogId">
            <column name="catlog_id" sql-type="int(11)"/>
        </id>
        <property name="catlogType">
            <column name="catlog_type" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>